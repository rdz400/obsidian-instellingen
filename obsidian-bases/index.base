filters:
  and:
    - file.ext == "base"
formulas:
  hoofdFolder: file.path.split('/')[0]
views:
  - type: table
    name: Table
    groupBy:
      property: formula.hoofdFolder
      direction: ASC
    order:
      - file.name
      - file.path
      - formula.hoofdFolder
      - file.backlinks
      - file.links
