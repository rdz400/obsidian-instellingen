filters:
  and:
    - file.folder != "0-dagelijks"
views:
  - type: table
    name: Table
    filters:
      and:
        - date >= (today() - duration('21d'))
    order:
      - file.name
      - date
      - file.folder
    sort:
      - property: date
        direction: DESC
    columnSize:
      file.name: 416
      note.date: 186
